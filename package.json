{
  "name": "ng-apps",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "start": "npm run dev",
    "predev": "tsc -p server",
    "dev": "concurrently \"ng serve --aot --ec true --open -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/index.js\"",
    "prod": "concurrently \"ng build --aot --prod && tsc -p server && node dist/server/index.js\"",
    "prod:build": "concurrently \"ng build --aot --prod && tsc -p server\"",
    "test": "ng test --browser ChromeHeadless --single-run",
    "testbe": "tsc -p server && mocha dist/server/tests --exit",
    "lintbe": "tslint server/**/**.ts{,x}",
    "watch": "ng test --browser ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.6",
    "@angular/common": "^5.2.6",
    "@angular/compiler": "^5.2.6",
    "@angular/core": "^5.2.6",
    "@angular/forms": "^5.2.6",
    "@angular/http": "^5.2.6",
    "@angular/platform-browser": "^5.2.6",
    "@angular/platform-browser-dynamic": "^5.2.6",
    "@angular/router": "^5.2.6",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cookie-parser": "latest",
    "core-js": "^2.5.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "errorhandler": "latest",
    "inversify": "^4.11.1",
    "method-override": "^2.3.10",
    "mocha": "^5.0.1",
    "mocha-typescript": "^1.1.12",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "typeorm": "^0.2.22",
    "winston": "^2.4.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "^5.2.6",
    "@angular/language-service": "^5.2.6",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.6",
    "@types/winston": "^2.3.8",
    "codelyzer": "~4.1.0",
    "concurrently": "^3.5.1",
    "cookie-parser": "latest",
    "jasmine-core": "~3.0.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "1.15.1",
    "protractor": "~5.3.0",
